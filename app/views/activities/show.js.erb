$('.modal').modal('hide');
$('#btn_group_overlay').addClass('hide');
$('#back_to_event').removeClass('hide');
$('#event_name').html("<%= j render partial: 'activities/activity_title', locals: {obj: @activity} %>");
$(".text_overlay_cover_pic").html("<%= escape_javascript( render partial: 'events/text_overlay', locals: {obj: @activity} ) %>");
$(".text_overlay_cover_pic").show();
$("#event_list").html("<%= escape_javascript(render partial: 'events/event_links', locals: { event: @event, activity: @activity } ) %>");
$('#show_event_div').html('<%= escape_javascript(render partial: "events/event_members", locals: { event: @event, activity: @activity, ad_users: @ad_users, inv_users: @inv_users}) %>');
$('.loading_page').hide();
$("#edit_del_btn").html("<%= escape_javascript(render :partial => 'events/activity', :locals => {:activty => @activity} ) %>");
$('#fancybox-loading').remove();
$('.fancybox-overlay').remove();