                <% if @events.count > 0 %>
                       <ul class='users' id='group_list'>
                      <div id="events_page" class="events_paginate">
                        <% @events.each do |event| %>
                          <%= render :partial => 'events/event', :locals => {:event => event} %> 
                        <% end %>
                        </div>
                      </ul>
                 <% else %>
                    <p class="semibold">No events to display</p>
                <% end %>